<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<VBox styleClass="about-container" stylesheets="@about.css" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.marvk.fs.vatsim.map.view.about.AboutView">
    <children>
        <Label styleClass="h1" text="Application"/>
        <Label styleClass="h2" text="Created by Name"/>
        <Label fx:id="version" text="Version"/>
        <Label onMouseClicked="#openSourceSoftwareDialog" text="Powered by Open Source Software"/>
    </children>
</VBox>
