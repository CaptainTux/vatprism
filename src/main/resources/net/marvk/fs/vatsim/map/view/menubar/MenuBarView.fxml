<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<HBox fx:id="container" styleClass="container" stylesheets="@MenuBarView.css" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.marvk.fs.vatsim.map.view.menubar.MenuBarView">
    <children>
        <MenuBar>
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <Region HBox.hgrow="ALWAYS"/>
        <Button mnemonicParsing="false" prefHeight="0.0" prefWidth="50.0" text="Minimize"/>
        <Button mnemonicParsing="false" prefHeight="0.0" prefWidth="50.0" text="Windowed"/>
        <Button mnemonicParsing="false" prefHeight="0.0" prefWidth="50.0" text="Close"/>
    </children>
</HBox>
